{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "^/favicon\\.png$",
      "dest": "/favicon.png"
    },
    {
      "src": "^/manifest\\.json$",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "^/service-worker\\.js$",
      "dest": "/service-worker.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Service-Worker-Allowed": "/",
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "^/icon-192\\.png$",
      "dest": "/icon-192.png"
    },
    {
      "src": "^/icon-512\\.png$",
      "dest": "/icon-512.png"
    },
    {
      "src": "^/apple-touch-icon\\.png$",
      "dest": "/apple-touch-icon.png"
    },
    {
      "src": "/api/restaurants",
      "dest": "api/restaurants/index.ts"
    },
    {
      "src": "/api/login",
      "dest": "api/login.ts"
    },
    {
      "src": "/api/admin/restaurants",
      "dest": "api/admin/restaurants/index.ts"
    },
    {
      "src": "/api/admin/restaurants/(?<id>[^/]*)",
      "dest": "api/admin/restaurants/[id].ts?id=$id"
    },
    {
      "src": "/api/admin/menu-items",
      "dest": "api/admin/menu-items/index.ts"
    },
    {
      "src": "/api/admin/menu-items/(?<id>[^/]*)",
      "dest": "api/admin/menu-items/[id].ts?id=$id"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
